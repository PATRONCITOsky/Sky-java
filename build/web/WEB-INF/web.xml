    <servlet>
        <servlet-name>ControladorPC</servlet-name>
        <servlet-class>controlador.ControladorPC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cproductos</servlet-name>
        <servlet-class>controlador.Cproductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cusu</servlet-name>
        <servlet-class>controlador.Cusu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cimagen</servlet-name>
        <servlet-class>controlador.Cimagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ccar</servlet-name>
        <servlet-class>controlador.Ccar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistroControl</servlet-name>
        <url-pattern>/RegistroControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validarIngreso</servlet-name>
        <url-pattern>/validarIngreso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorU</servlet-name>
        <url-pattern>/ControladorU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorP</servlet-name>
        <url-pattern>/ControladorP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPC</servlet-name>
        <url-pattern>/ControladorPC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cproductos</servlet-name>
        <url-pattern>/Cproductos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cusu</servlet-name>
        <url-pattern>/Cusu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cimagen</servlet-name>
        <url-pattern>/Cimagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ccar</servlet-name>
        <url-pattern>/Ccar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
